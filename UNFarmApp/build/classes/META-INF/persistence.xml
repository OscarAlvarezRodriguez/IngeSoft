<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="UNFarmAppPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>Entidad.Cliente</class>
    <class>Entidad.Compra</class>
    <class>Entidad.Domicilio</class>
    <class>Entidad.Drogueria</class>
    <class>Entidad.Empleado</class>
    <class>Entidad.Factura</class>
    <class>Entidad.Medicamento</class>
    <class>Entidad.MedicamentoComprado</class>
    <class>Entidad.MedicamentoVendido</class>
    <class>Pruebas.Medicamentoinvima</class>
    <class>Pruebas.Medicamento</class>
    <class>Pruebas.Compra</class>
    <class>Pruebas.Domicilio</class>
    <class>Pruebas.Factura</class>
    <class>Recursos.Medicamentoinvima</class>
    <class>Entidad.MedicamentoInvima</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/unfarmapp?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value=""/>
    </properties>
  </persistence-unit>
</persistence>
